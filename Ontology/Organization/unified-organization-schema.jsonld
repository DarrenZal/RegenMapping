    "email": "schema:email",
    "telephone": "schema:telephone",
    "contactPoint": "schema:contactPoint",
    "contactDetails": "murm:contact_details",
    
    "knowsAbout": {"@id": "schema:knowsAbout", "@container": "@set"},
